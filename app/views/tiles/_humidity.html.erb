
<div id="thing_<%= humidity.id %>" class="tile col-lg-2 col-md-3 col-sm-4 col-xs-6  zone_<%= humidity.zone_id %> item">
  <div class="dummy"></div>
    <div class="thumbnail">
        <div class="widget-title handle">
        <% if @location && @zone %>
          <% if current_page?(location_zone_path(@location, @zone)) %>
            <h4>Humidity</h4>
          <% end %>
        <% else %>
            <h4><%= link_to humidity.zone.name, location_zone_path(humidity.zone.location, humidity.zone) %></h4>
        <% end %>
        </div>
        <div class="temp humidity_<%= humidity.uid %> widget-profile-avatar">
          <%= humidity.device_value %> %
        </div>
         <h5><%= link_to humidity.label, thing_path(humidity) %></h5>
    </div>
</div>